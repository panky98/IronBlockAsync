<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>richorama,panky98</Authors>
    <PackageProjectUrl>https://github.com/panky98/IronBlockAsync</PackageProjectUrl>
    <License>MIT</License>
    <RepositoryUrl>https://github.com/panky98/IronBlockAsync</RepositoryUrl>
    <RepositoryType>github</RepositoryType>
    <PackageTags>blockly netcore scratch interpreter with async support</PackageTags>
    <Description>A .net core interpreter for blockly programs with support for async evaluation</Description>
    <Version>1.0.0</Version>
    <SignAssembly>true</SignAssembly>
    <AssemblyOriginatorKeyFile>keyPair.snk</AssemblyOriginatorKeyFile>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <RootNamespace>IronBlockAsync</RootNamespace>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.CodeAnalysis.CSharp.Scripting" Version="3.9.0" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\README.md" Pack="true" PackagePath="\"/>
  </ItemGroup>

</Project>
